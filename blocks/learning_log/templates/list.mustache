{{!
    @template block_learning_log/content

    Renders a list of learning events.

}}
<table class="list-wrapper">
<tr>
    <th>Date</th>
    <th>Event</th>
    <th>Action</th>
</tr>
{{#items}}
    <tr>
        {{#nesteditems}}
            {{>block_learning_log/item}}
        {{/nesteditems}}
    </tr>
{{/items}}
</table>
